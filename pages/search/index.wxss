page {
  background-color: #eeeeee;
  padding: 0 20rpx;
}
.search .search_box {
  width: 100%;
  height: 90rpx;
  /* padding: 0 15rpx; */
  display: flex;
  align-items: center;
}
.search .search_box input {
  flex: 1;
  height: 60rpx;
  padding-left: 30rpx;
  background-color: #ffffff;
}
.search .search_box button {
  width: 120rpx;
  height: 60rpx;
  font-size: 28rpx;
  margin-left: 20rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: #ffffff;
}
.search .search_list {
  margin-top: 20rpx;
  background-color: #ffffff;
}
.search .search_list .list_item {
  font-size: 32rpx;
  padding: 20rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  border-bottom: 1rpx solid #cccccc;
}

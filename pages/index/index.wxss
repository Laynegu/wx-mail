.index_swiper_wrap swiper {
  height: 340rpx;
}
.index_swiper_wrap image {
  width: 100%; 
}
.index_navBar {
  display: flex;
}
.index_navBar navigator {
  flex: 1;
  padding: 20rpx;
}
.index_navBar navigator image {
  width: 100%;
}
.index_floor .index_floor_title {
  margin: 10rpx 0 20rpx 0;
  padding-top: 15rpx;
  background-color: #f4f4f4;
}
.index_floor .index_floor_title image {
  width: 100%;
}
.index_floor .index_floor_list {
  padding: 0 20rpx;
  display: flex;
}
.index_floor .index_floor_list .left {
  width: 33%;
}
.index_floor .index_floor_list .right {
  flex: 1;
  display: flex;
  flex-wrap: wrap;
}
.index_floor_list .right navigator {
  width: 50%;
  padding: 0 0 8rpx 10rpx;
}
.index_floor .index_floor_list .product_img {
  width: 100%;
  height: 100%;
}
